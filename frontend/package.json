{
  "name": "near-explorer",
  "version": "0.0.1",
  "description": "NEAR blockchain explorer",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && env TZ=UTC jest",
    "dev": "next",
    "dev:local-wamp-testnet": "dotenv -c -e ../testnet.env -- next",
    "dev:local-wamp-mainnet": "dotenv -c -e ../mainnet.env -- next",
    "dev:production-wamp-testnet": "dotenv -c -e ../testnet.env -e ../production-wamp.env -- next",
    "dev:production-wamp-mainnet": "dotenv -c -e ../mainnet.env -e ../production-wamp.env -- next",
    "build": "next build",
    "start": "next start",
    "start:local-wamp-testnet": "dotenv -c -e ../testnet.env -- next start",
    "start:local-wamp-mainnet": "dotenv -c -e ../mainnet.env -- next start",
    "start:production-wamp-testnet": "dotenv -c -e ../testnet.env -e ../production-wamp.env -- next start",
    "start:production-wamp-mainnet": "dotenv -c -e ../testnet.env -e ../production-wamp.env -- next start",
    "e2e:test": "npm start & cypress run",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "e2e:record:parallel": "cypress run --record --parallel",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:ci": "start-server-and-test start:production-wamp-testnet 3000 cy:run",
    "compile-nextjs-config": "tsc --project nextjs.tsconfig.json",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/near-explorer.git"
  },
  "author": "Near Inc <hello@near.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/near/near-explorer/issues"
  },
  "homepage": "https://github.com/near/near-explorer#readme",
  "dependencies": {
    "@stitches/react": "^1.2.6",
    "autobahn": "^20.9.2",
    "babel-plugin-named-stitches-classnames": "^0.1.0",
    "bn.js": "^5.2.0",
    "bootstrap": "^4.6.1",
    "classnames": "^2.3.1",
    "country-data": "0.0.31",
    "d3": "^5.16.0",
    "dotenv-cli": "^5.0.0",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.1",
    "hexy": "^0.3.3",
    "i18next": "^21.6.3",
    "json-2-prom": "1.0.3",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.42.0",
    "moment": "^2.29.1",
    "near-api-js": "^0.44.1",
    "next": "^12.1.0",
    "patch-package": "^6.4.7",
    "rc-progress": "^3.1.4",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-countdown": "^2.3.2",
    "react-country-flag": "^3.0.2",
    "react-dom": "^17.0.2",
    "react-flip-move": "^3.0.3",
    "react-i18next": "^11.15.1",
    "react-infinite-scroll-component": "^5.1.0",
    "react-motion": "^0.5.2",
    "react-paginate": "^7.1.5",
    "react-text-collapse": "^0.5.2",
    "styled-components": "^4.4.0",
    "tslib": "^2.3.1",
    "tweetnacl": "^1.0.3",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.1.2",
    "@types/autobahn": "^18.10.1",
    "@types/bn.js": "^4.11.6",
    "@types/country-data": "0.0.2",
    "@types/d3": "^5.16.4",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.177",
    "@types/mixpanel-browser": "^2.35.7",
    "@types/node": "^12.20.37",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-paginate": "^7.1.1",
    "@types/react-test-renderer": "^17.0.1",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "autoprefixer": "^10.4.0",
    "cypress": "^8.7.0",
    "file-loader": "^3.0.1",
    "jest": "^27.4.4",
    "postcss": "^8.4.4",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "url-loader": "^1.1.2",
    "wait-on": "^3.3.0"
  }
}
