{
  "name": "@explorer/frontend",
  "version": "0.0.1",
  "description": "NEAR blockchain explorer frontend",
  "scripts": {
    "typecheck": "tsc --noEmit && npm run cy:typecheck",
    "test": "env TZ=UTC jest",
    "dev": "dotenv -c -e ../frontend.env -- next",
    "build": "next build",
    "start": "dotenv -c -e ../frontend.env -- next start",
    "e2e:test": "npm start & cypress run",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "e2e:record:parallel": "cypress run --record --parallel",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:typecheck": "tsc --project cypress/tsconfig.json --noEmit",
    "compile-nextjs-config": "ncc build next.config-prod.js --transpile-only --tsconfig-path nextjs.tsconfig.json --out . && mv index.js next.config.js",
    "postinstall": "patch-package",
    "plugin:test": "ava"
  },
  "ava": {
    "files": [
      "utils/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/near-explorer.git"
  },
  "author": "Near Inc <hello@near.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/near/near-explorer/issues"
  },
  "homepage": "https://github.com/near/near-explorer#readme",
  "dependencies": {
    "@stitches/react": "^1.3.1-1",
    "@trpc/client": "^9.27.4",
    "@trpc/next": "^9.27.4",
    "@trpc/react": "^9.27.4",
    "@types/analytics-node": "^3.1.9",
    "analytics-node": "^6.2.0",
    "bootstrap": "^4.6.1",
    "classnames": "^2.3.1",
    "country-data": "0.0.31",
    "d3": "^5.16.0",
    "date-fns": "^2.28.0",
    "dotenv-cli": "^5.0.0",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.1",
    "hexy": "^0.3.3",
    "i18next": "^21.6.3",
    "jsbi": "^4.3.0",
    "json-2-prom": "1.0.3",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "next-i18next": "^12.1.0",
    "rc-progress": "^3.1.4",
    "rc-switch": "^4.0.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-copy-to-clipboard": "^5.1.0",
    "react-countdown": "^2.3.2",
    "react-country-flag": "^3.0.2",
    "react-dom": "^17.0.2",
    "react-flip-move": "^3.0.3",
    "react-hot-toast": "^2.2.0",
    "react-i18next": "^11.15.1",
    "react-image": "^4.0.3",
    "react-infinite-scroll-component": "^5.1.0",
    "react-json-view": "^1.21.3",
    "react-paginate": "^7.1.5",
