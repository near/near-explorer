{
  "name": "near-explorer",
  "version": "0.0.1",
  "description": "NEAR blockchain explorer",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && jest",
    "dev": "next",
    "dev:local-wamp": "env NEAR_EXPLORER_DATA_SOURCE=INDEXER_BACKEND env WAMP_NEAR_EXPLORER_URL=ws://localhost:8080/ws npm run dev",
    "dev:local-wamp-testnet": "env 'NEAR_NETWORKS=[{\"name\": \"testnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.testnet.near.org/profile\"}]' npm run dev:local-wamp",
    "dev:local-wamp-mainnet": "env 'NEAR_NETWORKS=[{\"name\": \"mainnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.near.org/profile\"}]' npm run dev:local-wamp",
    "dev:production-wamp": "env WAMP_NEAR_EXPLORER_URL=wss://near-explorer-wamp.onrender.com/ws env NEAR_EXPLORER_DATA_SOURCE=INDEXER_BACKEND npm run dev",
    "dev:production-wamp-testnet": "env 'NEAR_NETWORKS=[{\"name\": \"testnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.testnet.near.org/profile\"}]' npm run dev:production-wamp",
    "dev:production-wamp-mainnet": "env 'NEAR_NETWORKS=[{\"name\": \"mainnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.near.org/profile\"}]' npm run dev:production-wamp",
    "build": "next build",
    "start": "next start",
    "start:local-wamp": "env WAMP_NEAR_EXPLORER_URL=ws://localhost:8080/ws env NEAR_EXPLORER_DATA_SOURCE=INDEXER_BACKEND npm run start",
    "start:local-wamp-testnet": "env 'NEAR_NETWORKS=[{\"name\": \"testnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.testnet.near.org/profile\"}]' npm run start:local-wamp",
    "start:local-wamp-mainnet": "env 'NEAR_NETWORKS=[{\"name\": \"mainnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.near.org/profile\"}]' npm run start:local-wamp",
    "start:production-wamp": "env WAMP_NEAR_EXPLORER_URL=wss://near-explorer-wamp.onrender.com/ws env NEAR_EXPLORER_DATA_SOURCE=INDEXER_BACKEND npm run start",
    "start:production-wamp-testnet": "env 'NEAR_NETWORKS=[{\"name\": \"testnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.testnet.near.org/profile\"}]' npm run start:production-wamp",
    "start:production-wamp-mainnet": "env 'NEAR_NETWORKS=[{\"name\": \"mainnet\", \"explorerLink\": \"http://localhost:3000/\", \"aliases\": [\"localhost:3000\", \"127.0.0.1:3000\"], \"nearWalletProfilePrefix\": \"https://wallet.near.org/profile\"}]' npm run start:production-wamp",
    "e2e:test": "npm start & cypress run",
    "e2e": "cypress run",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:record": "cypress run --record",
    "e2e:record:parallel": "cypress run --record --parallel",
    "cy:verify": "cypress verify",
    "cy:version": "cypress version",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:ci": "start-server-and-test start:production-wamp-testnet 3000 cy:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/near-explorer.git"
  },
  "author": "Near Inc <hello@near.org>",
  "license": "Apache-2.0",
  "bugs": {
