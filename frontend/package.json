    "@trpc/next": "^10.26.0",
    "@trpc/react-query": "^10.26.0",
    "@trpc/server": "^10.26.0",
    "@types/analytics-node": "^3.1.9",
    "analytics-node": "^6.2.0",
    "bootstrap": "^4.6.1",
