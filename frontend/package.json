    "country-data": "0.0.31",
    "d3": "^5.16.0",
    "date-fns": "^2.28.0",
    "dotenv-cli": "^5.0.0",
    "echarts": "^5.2.2",
    "echarts-for-react": "^3.0.1",
    "hexy": "^0.3.3",
    "i18next": "^22.4.15",
    "i18next-chained-backend": "^4.2.0",
    "i18next-http-backend": "^2.2.0",
    "i18next-localstorage-backend": "^4.1.1",
    "jsbi": "^4.3.0",
    "json-2-prom": "1.0.3",
