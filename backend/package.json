{
  "name": "near-explorer-backend",
  "version": "0.0.1",
  "description": "NEAR blockchain explorer backend",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "env NODE_ENV=production node src/index.js",
    "start:testnet": "env NEAR_RPC_URL=https://rpc.testnet.nearprotocol.com NEAR_DB_PREFIX=testnet NEAR_GENESIS_RECORDS_URL=https://s3-us-west-1.amazonaws.com/build.nearprotocol.com/nearcore-deploy/testnet/genesis.json npm run start",
    "start:betanet": "env NEAR_RPC_URL=https://rpc.betanet.nearprotocol.com NEAR_DB_PREFIX=betanet NEAR_GENESIS_RECORDS_URL=https://s3-us-west-1.amazonaws.com/build.nearprotocol.com/nearcore-deploy/betanet/genesis.json npm run start",
    "start:mainnet": "env NEAR_RPC_URL=https://rpc.mainnet.nearprotocol.com NEAR_DB_PREFIX=mainnet NEAR_GENESIS_RECORDS_URL=https://raw.githubusercontent.com/nearprotocol/nearcore/master/neard/res/mainnet_genesis.json npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/near/near-explorer.git"
  },
  "author": "Near Inc <hello@near.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/near/near-explorer/issues"
  },
  "homepage": "https://github.com/near/near-explorer#readme",
  "dependencies": {
    "autobahn": "19.4.1",
    "geoip-lite": "^1.4.2",
    "moment": "^2.24.0",
    "near-api-js": "^0.27.0",
    "pg": "^8.3.0",
    "sequelize": "^5.21.1",
    "sqlite3": "^4.1.0",
    "stream-json": "^1.7.1",
    "ws": "^7.2.0"
  }
}
